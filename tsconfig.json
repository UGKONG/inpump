{
  "compilerOptions": {
    "target": "ES2016",
    "module": "CommonJS",
    "jsx": "react-native",
    "strict": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "typeRoots": [
      "@types",
      "node_modules/@types"
    ]
  }
}